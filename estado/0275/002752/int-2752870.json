            "cac:AwardingTerms": {
                "cac:AwardingCriteria": [
                    {
                        "cbc:Description": "Que los equipos aportaos por el licitador dispongan de certificados UNE-EN 61610:1997 \u00f3 UNE-EN 61966-7-1:2007 (calidad de la imagen y el color)",
                        "cbc:WeightNumeric": "3"
                    },
                    {
                        "cbc:Description": "Los equipos aportados por el licitador cumplen con certificaciones o protocolos de seguridad de las comunicaciones UNE-EN ISO/IEC 27001:2017 (ISO/IEC 27001:2013, incluyendo COR 1:2014 y COR 2:2015), I",
                        "cbc:WeightNumeric": "3"
                    },
                    {
                        "cbc:Description": "Que el consumo el\u00e9ctrico t\u00edpico CET, de los equipos aportados por el licitador (Kwh/semana) sea de 2 Kwh o inferior, o cumplan con la norma UNE-EN ISO 50001:2011.",
                        "cbc:WeightNumeric": "3"
                    },
                    {
                        "cbc:Description": "Aportaci\u00f3n por parte del licitador de equipos nuevos o usados.",
                        "cbc:WeightNumeric": "5"
                    },
                    {
                        "cbc:Description": "Que los equipos aportados por el licitador dispongan de certificados UNE EN 55204:2011 \u00f3 UNE-EN 55204:2011/A1:2015 (equipos de tecnolog\u00eda de la informaci\u00f3n. Caracter\u00edsticas de inmunidad).",
                        "cbc:WeightNumeric": "3"
                    },
                    {
                        "cbc:Description": "Que el personal t\u00e9cnico encargado del mantenimiento/reparaci\u00f3n de los equipos en la empresa tenga m\u00e1s de 10 a\u00f1os de experiencia",
                        "cbc:WeightNumeric": "9"
                    },
                    {
                        "cbc:Description": "Que los equipos aportados por el licitador dispongan de etiquetas tipo ENERGY STAR, BLUE ANGEL, CISNE BLANCO, o equivalente.",
                        "cbc:WeightNumeric": "3"
                    },
                    {
                        "cbc:Description": "Que el licitador acredite su condici\u00f3n de Servicio T\u00e9cnico Oficial de marcas registradas de los equipos a utilizar.",
                        "cbc:WeightNumeric": "6"
                    },
                    {
                        "cbc:Description": "Que el licitador vaya a contratar al menos una persona que sea mujer o discapacitada, para la ejecuci\u00f3n del servicio.",
                        "cbc:WeightNumeric": "4"
                    },
                    {
                        "cbc:Description": "Precio",
                        "cbc:WeightNumeric": "55"
                    },
                    {
                        "cbc:Description": "Que el nivel de ruido de los equipos aportados por el licitador cumple con la norma UNE-EN ISO-7779:2011/2010, o ISO 9296:2017.",
                        "cbc:WeightNumeric": "3"
                    },
                    {
                        "cbc:Description": "Acreditaci\u00f3n de que los equipos aportados por el licitador est\u00e1n fabricados con biopl\u00e1sticos o pl\u00e1sticos reciclados.",
                        "cbc:WeightNumeric": "3"
                    }
                ]
            },
